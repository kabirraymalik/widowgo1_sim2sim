<mujoco model="widowGo1">
  <compiler angle="radian"/>
  <option cone="elliptic" impratio="100"/>

  <default>
    <default class="robot">
      <default class="motor">
        <joint />
        <motor />
      </default>
      <!-- Go1-like joint stabilizers -->
      <joint damping="2" armature="0.01" frictionloss="0.2"/>

      <!-- Visuals: render-only, group 0 -->
      <default class="visual">
        <geom contype="0" conaffinity="0" group="0"/>
      </default>

      <!-- Collisions: physics-only, group 3 (transparent by default) -->
      <default class="collision">
        <geom material="collision_material" group="3"
              contype="1" conaffinity="1"
              margin="0.001" condim="1" priority="1"
              friction="0.6 0.02 0.01"/>
      </default>
    </default>
  </default>

  <asset>
    <material name="black"  rgba="0.0 0.0 0.0 1.0"/>
    <material name="blue"   rgba="0.0 0.0 0.8 1.0"/>
